{
  "$schema": "https://stryker-mutator.io/schema/stryker-schema.json",
  "mutator": "javascript",
  "testRunner": "command",
  "commandRunner": {
    "command": "npx qunit"
  },
  "reporters": ["html", "clear-text", "progress"],
  "coverageAnalysis": "off",
  "files": ["src/**/*.js", "tests/**/*.js", "test/**/*.cjs"],
  "mutate": ["src/**/*.js"]
}
